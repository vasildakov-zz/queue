{
    "name": "vasildakov/queue",
    "description": "Queue",
    "type": "Library",
    "license": "MIT",
    "authors": [
        {
            "name": "Vasil Dakov",
            "email": "vasildakov@gmail.com"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.1",
        "roave/security-advisories": "dev-master",
        "container-interop/container-interop": "^1.1",
        "pda/pheanstalk": "^3.1",
        "monolog/monolog": "^1.22",
        "guzzlehttp/guzzle": "^6.2",
        "aws/aws-sdk-php-resources": "^0.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "squizlabs/php_codesniffer": "^2.8",
        "filp/whoops": "^1.1 || ^2.0",
        "satooshi/php-coveralls": "^1.0",
        "phpunit/php-code-coverage": "^5.0",
        "phpmetrics/phpmetrics": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Queue\\": "src/Queue"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "QueueTest\\": "tests/Queue"
        }
    },
    "config": {
        "platform": {
            "php": "7.1.0"
        }
    }
}
